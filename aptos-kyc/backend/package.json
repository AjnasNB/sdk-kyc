{
    "name": "aptos-kyc-backend",
    "version": "1.0.0",
    "description": "Backend API service for Aptos KYC system",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "zk:setup": "cd src/zk && bash setup.sh",
        "zk:prove": "ts-node src/zk/generateProof.ts",
        "test": "jest",
        "clean": "rm -rf dist node_modules"
    },
    "dependencies": {
        "aptos": "^1.12.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "joi": "^17.11.0",
        "mongoose": "^8.0.3",
        "multer": "^1.4.5-lts.1",
        "snarkjs": "^0.7.3",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/uuid": "^9.0.7",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "aptos",
        "kyc",
        "blockchain",
        "identity"
    ],
    "author": "Cognifyr",
    "license": "MIT"
}
